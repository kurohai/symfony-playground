<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <meta name="description" content="">
        <meta name="author" content="">

        <title>{% block title %}Evan + Symfony{% endblock %}</title>

        {% block stylesheets %}
        <style>
            @media (min-width: 768px) {
                #wrapper { width: 80%; margin: 2em auto; }
                #icon-book { display: inline-block; }
                #status a, #next a { display: block; }

                @-webkit-keyframes fade-in { 0% { opacity: 0; } 100% { opacity: 1; } }
                @keyframes fade-in { 0% { opacity: 0; } 100% { opacity: 1; } }
                .sf-toolbar { opacity: 0; -webkit-animation: fade-in 1s .2s forwards; animation: fade-in 1s .2s forwards;}
            }
        </style>

        {% endblock %}

        <script type="text/javascript" src="/lib/browser.js"></script>
        <script type="text/javascript" src="/lib/react-15.2.1.js"></script>
        <script type="text/javascript" src="/lib/react-dom-15.2.1.js"></script>
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="stylesheet" href="/lib/bootstrap.css">
        <link rel="stylesheet" href="/lib/sb-admin.css">
        <link rel="stylesheet" href="/lib/font-awesome.css">
    </head>
    <body>
<!--         {% include 'navbar.html.twig' %}
 -->        <div class="app-content">

        {% block fos_user_content %}{% include 'navbar.html.twig' %}{% endblock %}
        {% block body %}{% endblock %}
        </div>
        <!-- <script type="text/javascript" src="lib/jquery.js"></script> -->
        <script type="text/javascript" src="/lib/jquery-3.1.0.js"></script>
        <script type="text/javascript" src="/lib/bootstrap.js"></script>
        <!-- <script type="text/javascript" src="/js/window-sizing.js"></script> -->
        {% block javascripts %}{% endblock %}
        <link rel="stylesheet" href="/css/base-app.css">
        <link rel="stylesheet" href="/css/dev-toolbar.css">

    </body>
</html>
